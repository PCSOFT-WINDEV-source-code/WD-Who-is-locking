     procedure_id : 1256931447729984088
     type_code : 14
     code : |1+
      Procedure RedisplayLengths()
      
      // Re-calculates the durations for the visible lines
      // of the browsing table (no network access)
      
      FOR ALL ROW of TABLE
      	CalcDuration(TABLE)
      END
      
