    name : BTN_Check
    identifier : 0x2171847e075f9258
    internal_properties : 
    properties :
     x : 12
     y : 158
     width : 125
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         IF NOT YesNo(No,<§$0001§>) THEN RETURN   
         //2001:In case of abnormal application closing, the HyperFileSQL engine automatically unlocks the records...
         
         // This routine checks whether no information is still stored 
         // in the "Lock" file whilst the unlocking took place 
         // (Abnormal closing of the application)
         
         EndTimerSys(1)
         
         // Browse the "Lock" file sequentially
         i is int  
         nNbCorrections is int  
         LOOP
         	i++
         	LOOP
         		HRead(LOCK,i)
         		IF HErrorLock() THEN
         			Multitask(200) 	//	Retry 2 secs later
         		ELSE
         			BREAK
         		END
         	END
         	IF HOut() THEN BREAK
         	IF (HState()=hStateActive) THEN
         		// Checks whether the record is effectively locked
         		HRead(LOCK.FILES,LOCK.REC,hLockWrite)
         		IF NOT HErrorLock() THEN
         			// Deletes the information from the "Lock" file
         			LOOP
         				HDelete(LOCK)
         				IF HErrorLock() THEN
         					Multitask(200) 	//	Retry 2 secs later
         				ELSE
         					BREAK
         				END
         			END
         			nNbCorrections++
         		END
         	END
         END
         TableDisplay(TABLE,taStart)
         TimerSys(Refresh,50,1)
         
         SWITCH nNbCorrections
         	CASE 0
         		Info(<§$000b§>,...
         		<§$000c§>)
         	CASE 1
         		Info(<§$000d§>,...
         		<§$000e§>)
         	OTHER CASE
         		Info(nNbCorrections+<§$000f§>,...
         		<§$0010§>)
         END
         
        type : 18
      -
        type : 161
        enabled : false
