    name : BTN_Delete
    identifier : 0x2171847e07619258
    internal_properties : 
    properties :
     x : 12
     y : 190
     width : 125
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         IF TABLE..Empty THEN
         	Beep
         	RETURN
         END
         
         	
         IF YesNo(No,StringBuild(<§$0012§> + CR +...
         <§$0013§>,LOCK.USER,COL_Duration,LOCK.USER)) THEN
         	EndTimerSys(1)
         	// Message for the user who is modifying the record
         	sMsgToSend is string 
         	sMsgToSend = Open(WIN_EditMessage,	<§$0014§>,LOCK.USER)
         	IF sMsgToSend<>"" THEN
         		// Message for the user who is modifying the record
         		LOCK.SENDMSG = True
         		LOCK.MSG = sMsgToSend
         		LOCK.MSGAUTHOR = NetworkUser()
         		LOOP
         			HModify(LOCK)
         			IF HErrorLock() THEN
         				Multitask(200) 	//	Retry 2 secs later
         			ELSE
         				BREAK
         			END
         		END
         		
         	END
         	IF HError(hErrCurrent) = 0 THEN
         		TimerSys(Refresh,50,1)		
         	ELSE
         		// An error other than a lock error occurred
         		// Display an error message
         		Error(<§$0016§>,<§$0017§> + HErrorInfo())
         	END
         END
         
