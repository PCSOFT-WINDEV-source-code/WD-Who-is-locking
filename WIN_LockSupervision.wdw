     name : CalcDuration
     internal_properties : CAAAAAgAAADkNP5d2hLQKr7pikhgXI8/ya6ffSP+b2TikzOzb8vXzUQvdpoa34wJvFw2CIyZd3zLgwTUxTl2sDObZqBnrTahCz87bqJIveo+JqwGxtIpfsR3B5rJBfJPL9Y8S1IVY86gTxa/qhVRY0TJkvlyLzo/8oTzXlecmiXGreW89IM78M4FCPuvG4NWhYCiEye9dtoVVOid
     procedure_id : 1256931447729853016
     type_code : 14
     code : |1+
      Procedure CalcDuration(i)
      
      // Calculates the duration of the lock for the requested line
      
      WHEN EXCEPTION
          // Ignores the error if invalid date
          COL_Duration[i] = "?"
      	RETURN
      END
      
      // Calculate the duration
      sDiff is string = DateTimeDifference(COL_StartDate[i]+COL_StartTime[i],Today()+Now()[[1 TO 6]])
      // Display the time passed
      sDisplayedTxt is string
      
      IF Val(sDiff[[2 TO 8]])>0 THEN sDisplayedTxt += Val(sDiff[[2 TO 8]])+" d. " // Days (even if unlikely)
      
      IF sDisplayedTxt~="" 	// < 1 day => check whether hours
      	IF Val(Middle(sDiff,9,2))>0 THEN sDisplayedTxt += Middle(sDiff,9,2)+" h " // Hours (possible?)
      ELSE				// > 1 day=> hours always displayed
      	sDisplayedTxt += Middle(sDiff,9,2)+" h " // Hours (possible?)		
      END
      
      sDisplayedTxt +=  Middle(sDiff,11,2)+" ' " + Middle(sDiff,13,2) + " '' "
      COL_Duration[i] = sDisplayedTxt
      
