    name : BTN_Unlock
    identifier : 0x2171847e07609258
    internal_properties : 
    properties :
     x : 12
     y : 222
     width : 125
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : CAAAAAgAAABPKR6VVcL8snuAm24Yo8BZvyFEEPjRoyeTfu/EJLHnB0WobozrdMmhbC8fEDQChS5jnvJ8AHhQghhHDrNi7ozuRy9Sof+iHg6uqJiymODmNMWH1onsOeyNWip7AgFn1X6/s61fXL8WjkCSs+g=
        code : |1+
         IF TABLE..Empty THEN
         	Beep
         	RETURN
         END
         
         IF NOT YesNo(No,<§$0002§>) THEN RETURN
         
         
         EndTimerSys(1)
         
         // Checks whether the record is effectively locked
         HRead(LOCK.FILES,LOCK.REC,hLockWrite)
         IF NOT HErrorLock() THEN
         	// Deletes the information from the "Lock" file
         	LOOP
         		HDelete(LOCK)
         		IF HErrorLock() THEN
         			Multitask(200) 	//	Retry 2 secs later
         		ELSE
         			BREAK
         		END
         	END
         ELSE
         	// Checks whether the record still exists
         	HRead(LOCK.FILES,LOCK.REC)
         	IF (HState()=hStateActive)
         		// Forces the flag to ask the corresponding application to close
         		// (and therefore to unlock the corresponding record)
         		MsgToSend is string 
         		MsgToSend = Open(WIN_EditMessage,	<§$0011§>,LOCK.USER)
         		IF MsgToSend<>"" THEN
         			LOCK.SENDMSG = 9
         			LOCK.MSG = MsgToSend
         			LOCK.MSGAUTHOR = NetworkUser()
         			LOOP
         				HModify(LOCK)
         				IF HErrorLock() THEN
         					Multitask(200) 	//	Retry 2 secs later
         				ELSE
         					BREAK
         				END
         			END
             	END
         	END
         END
         TimerSys(Refresh,50,1)
         
        type : 18
