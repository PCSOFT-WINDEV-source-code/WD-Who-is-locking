     code : |1+
      Procedure Refresh()
      
      // To automatically detect the modifications performed on the other computers.
      
      EndTimerSys(1)
      IF gnFileVersion <> HVersion(LOCK) THEN
      	// Modification made on another computer
      	gnFileVersion = HVersion(LOCK)
      	TableDisplay(TABLE,taStart)
      	IF TABLE..Empty THEN TABLE..ToolTip=""
      END
      TimerSys(Refresh,50,1)
      
      RedisplayLengths()
      
